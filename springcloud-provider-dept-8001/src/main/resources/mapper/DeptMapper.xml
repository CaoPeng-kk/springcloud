<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.caop.springcloud.mapper.DeptMapper" >

    <select id="selectDept" resultType="com.caop.springcloud.pojo.Dept">
        select dept_id, dept_name, db_source from dept where dept_id = #{id}
    </select>
</mapper>